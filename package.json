{
  "name": "@thommf-portfolio/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "setup": "./infra/scripts/setup_oidc.sh",
    "dev": "nx run-many --target=serve --projects=container,headerMfe,projectListMfe",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --exclude=\"*-e2e\"",
    "lint": "nx run-many --target=lint --all",
    "test": "nx run-many --target=test --all",
    "test:e2e": "nx run-many --target=e2e --all",
    "test:e2e:dev": "nx run-many --target=e2e --all -- --ui",
    "test:e2e:docker": "docker compose -f ./infra/e2e/docker-compose.e2e.yml up --build --abort-on-container-exit --remove-orphans --attach e2e-tests",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "prettier": "nx format:write",
    "deploy": "./infra/deploy.sh",
    "deploy:infra": "./infra/deploy.sh infrastructure-only",
    "deploy:build": "./infra/deploy.sh build-only",
    "deploy:upload": "./infra/deploy.sh upload-only",
    "run:prod": "docker compose up --build",
    "tf:init": "terraform -chdir=infra/terraform init -upgrade -reconfigure -backend-config=backend-${TF_ENV:-local}.hcl",
    "tf:plan": "terraform -chdir=infra/terraform plan -refresh=true -input=false -out=tfplan -var-file=terraform-${TF_ENV:-local}.tfvars",
    "tf:apply": "terraform -chdir=infra/terraform apply -input=false tfplan"
  },
  "private": true,
