{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/jest",
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "es6"],
    "types": [
      "jest",
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ]
  },
  "include": [
    "jest.config.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.tsx",
    "src/**/*.test.js",
    "src/**/*.spec.js",
    "src/**/*.test.jsx",
    "src/**/*.spec.jsx",
    "src/**/*.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "../../libs/shared/config/tsconfig.lib.json"
    },
    {
      "path": "../../libs/store/tsconfig.lib.json"
    },
    {
      "path": "../../libs/portfolio-store/tsconfig.lib.json"
    }
  ]
}
